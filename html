                    </button>
                    <button class="tab-btn" role="tab" aria-selected="false" aria-controls="terraform-panel" data-tab="terraform">
                        <i class="fas fa-layer-group" aria-hidden="true"></i> Terraform
                    </button>
                </div>

                <div class="code-showcase" data-aos="fade-up" data-aos-delay="400">
                    <!-- Jenkins Pipeline -->
                    <div class="code-panel active" id="jenkins-panel" role="tabpanel" aria-labelledby="jenkins-tab">
                        <div class="code-block">
                            <div class="code-header">
                                <div class="code-title">
                                    <i class="fab fa-jenkins" aria-hidden="true"></i>
                                    <span>Advanced Jenkinsfile</span>
                                </div>
                                <div class="code-actions">
                                    <button class="code-action" data-action="copy" aria-label="Copy code">
                                        <i class="fas fa-copy"></i>
                                    </button>
                                    <button class="code-action" data-action="fullscreen" aria-label="Fullscreen view">
                                        <i class="fas fa-expand"></i>
                                    </button>
                                    <span class="code-language">Groovy</span>
                                </div>
                            </div>
                            <pre><code class="language-groovy">pipeline {
    agent any
    
    environment {
        DOCKER_REGISTRY = 'registry.company.com'
        APP_NAME = 'devops-demo'
        KUBECONFIG = credentials('k8s-config')
        SLACK_CHANNEL = '#deployments'
    }
    
    stages {
        stage('Parallel Checkout & Setup') {
            parallel {
                stage('Source Checkout') {
                    steps {
                        checkout scm
                        sh 'git submodule update --init --recursive'
                    }
                }
                stage('Environment Setup') {
                    steps {
                        sh 'docker system prune -f'
                        sh 'kubectl cluster-info'
                    }
                }
            }
        }
        
        stage('Security & Quality Scans') {
            parallel {
                stage('SAST Scan') {
                    steps {
                        sh 'sonar-scanner -Dsonar.projectKey=$APP_NAME'
                    }
                }
                stage('Dependency Check') {
                    steps {
                        sh 'npm audit --audit-level=high'
                        sh 'snyk test --severity-threshold=high'
                    }
                }
                stage('Infrastructure Scan') {
                    steps {
                        sh 'terraform plan -out=tfplan'
                        sh 'tfsec tfplan'
                    }
                }
            }
        }
        
        stage('Multi-Architecture Build') {
            steps {
                script {
                    def buildArgs = [
                        '--platform=linux/amd64,linux/arm64',
                        '--build-arg VERSION=${BUILD_NUMBER}',
                        '--build-arg COMMIT_SHA=${GIT_COMMIT}',
                        '--cache-from=type=registry,ref=${DOCKER_REGISTRY}/${APP_NAME}:cache',
                        '--cache-to=type=registry,ref=${DOCKER_REGISTRY}/${APP_NAME}:cache,mode=max',
                        '-t ${DOCKER_REGISTRY}/${APP_NAME}:${BUILD_NUMBER}',
                        '-t ${DOCKER_REGISTRY}/${APP_NAME}:latest',
                        '--push .'
                    ].join(' ')
                    
                    sh "docker buildx build ${buildArgs}"
                }
            }
        }
        
        stage('Container Security Scan') {
            steps {
                sh 'trivy image --severity HIGH,CRITICAL ${DOCKER_REGISTRY}/${APP_NAME}:${BUILD_NUMBER}'
                sh 'docker run --rm -v /var/run/docker.sock:/var/run/docker.sock aquasec/trivy config .'
            }
        }
        
        stage('Automated Testing') {
            parallel {
                stage('Unit Tests') {
                    steps {
                        sh 'npm run test:unit -- --coverage --watchAll=false'
                        publishTestResults testResultsPattern: 'test-results.xml'
                    }
                }
                stage('Integration Tests') {
                    steps {
                        sh 'npm run test:integration'
                    }
                }
                stage('E2E Tests') {
                    steps {
                        sh 'npm run test:e2e -- --headless'
                    }
                }
            }
        }
        
        stage('Staging Deployment') {
            steps {
                script {
                    sh '''
                        helm upgrade --install ${APP_NAME}-staging ./helm-chart \\
                            --namespace staging \\
                            --set image.tag=${BUILD_NUMBER} \\
                            --set environment=staging \\
                            --wait --timeout=300s
                    '''
                    
                    // Health check
                    sh 'kubectl wait --for=condition=ready pod -l app=${APP_NAME} -n staging --timeout=300s'
                }
            }
        }
        
        stage('Smoke Tests') {
            steps {
                sh 'npm run test:smoke -- --env=staging'
            }
        }
        
        stage('Production Deployment') {
            when {
                branch 'main'
            }
            steps {
                script {
                    // Blue-Green Deployment
                    sh '''
                        # Deploy to green environment
                        helm upgrade --install ${APP_NAME}-green ./helm-chart \\
                            --namespace production \\
                            --set image.tag=${BUILD_NUMBER} \\
                            --set environment=production-green \\
                            --wait --timeout=600s
                        
                        # Switch traffic gradually
                        kubectl patch service ${APP_NAME} -p '{"spec":{"selector":{"version":"green"}}}'
                        
                        # Wait and verify
                        sleep 30
                        curl -f https://api.company.com/health || exit 1
                        
                        # Remove blue environment
                        helm uninstall ${APP_NAME}-blue || true
                    '''
                }
            }
        }
    }
    
    post {
        always {
            // Cleanup
            sh 'docker system prune -f'
            
            // Archive artifacts
            archiveArtifacts artifacts: 'dist/**/*', allowEmptyArchive: true
            
            // Publish reports
            publishHTML([
                allowMissing: false,
                alwaysLinkToLastBuild: true,
                keepAll: true,
                reportDir: 'coverage',
                reportFiles: 'index.html',
                reportName: 'Coverage Report'
            ])
        }
        
        success {
            slackSend(
                channel: env.SLACK_CHANNEL,
                color: 'good',
                message: "✅ Deployment successful! ${APP_NAME}:${BUILD_NUMBER} is now live."
            )
        }
        
        failure {
            slackSend(
                channel: env.SLACK_CHANNEL,
                color: 'danger',
                message: "❌ Deployment failed! ${APP_NAME}:${BUILD_NUMBER} - Check logs."
            )
        }
    }
}</code></pre>
                        </div>
                    </div>

                    <!-- Docker Configuration -->
