        <section class="code-demo" aria-labelledby="code-title">
            <div class="container">
                <h2 id="code-title" class="section-title" data-aos="fade-up">
                    Enterprise DevOps Architecture
                    <span class="title-accent">Infrastructure as Code</span>
                </h2>
                
                <!-- Code Tab Navigation -->
                <div class="code-tabs" role="tablist" aria-label="Code examples" data-aos="fade-up" data-aos-delay="200">
                    <button class="tab-btn active" role="tab" aria-selected="true" aria-controls="jenkins-panel" data-tab="jenkins">
                        <i class="fab fa-jenkins" aria-hidden="true"></i> Jenkins Pipeline
                    </button>
